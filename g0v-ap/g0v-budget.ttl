#################################################################


###  http://data.budget.g0v.it/g0v-budget/v1#Account

:Account rdf:type owl:Class ;
         
         rdfs:subClassOf qb:Observation ;
         
         rdfs:comment "An observation of a money amount related to a government budget or finatial report"@en .



###  http://data.budget.g0v.it/g0v-budget/v1#Budget

:Budget rdf:type owl:Class ;
        
        rdfs:subClassOf :Account ;
        
        rdfs:comment "An observed forecast amount on a n account"@en .



###  http://data.budget.g0v.it/g0v-budget/v1#BudgetTaxonomy

:BudgetTaxonomy rdf:type owl:Class ;
                
                rdfs:subClassOf skos:ConceptScheme .



###  http://data.budget.g0v.it/g0v-budget/v1#Dataset

:Dataset rdf:type owl:Class ;
         
         rdfs:label "Dataset"@en ;
         
         rdfs:subClassOf qb:Dataset ,
                         dcat:Dataset ;
         
         rdfs:comment "A budget datasource document containing  goverment budget related  data."@en .



###  http://data.budget.g0v.it/g0v-budget/v1#Record

:Record rdf:type owl:Class ;
        
        rdfs:subClassOf :Account ;
        
        rdfs:comment "A record of and historic value regarding an account"@en .



###  http://data.budget.g0v.it/g0v-budget/v1#RecordTaxonomy

:RecordTaxonomy rdf:type owl:Class ;
                
                rdfs:subClassOf skos:ConceptScheme .



