    <!-- http://data.budget.g0v.it/g0v-budget/v1#byteCount -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#byteCount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">byte count</rdfs:label>
        <rdfs:comment xml:lang="en">is the number of bytes processed by an ingestion activity.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#constructQuery -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#constructQuery">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">construct query</rdfs:label>
        <rdfs:comment xml:lang="en">The SPARQL 1.1.  query to extract data from a SPARQL endpoint. No paging constructs allowed ( OFSET and LIMIT ), use pageSize, fromPage and toPage instead. If not specified &quot;CONSTRUCT { ?s ?p ?o }  WHERE { ?s ?p ?o }&quot; apply</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#createsGraphName -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#createsGraphName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>creates graph name</rdfs:label>
        <rdfs:comment xml:lang="en">The related uri is used as as the name of the graph produced according the described accrual policy. If not specified the same value of g0v-budget:url is inferred.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#executionPriority -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#executionPriority">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">execution priority</rdfs:label>
        <rdfs:comment xml:lang="en">An optional integer that determine the ordere of execution of a rule ( ascending).</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#expectedParts -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#expectedParts">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">expected parts</rdfs:label>
        <rdfs:comment xml:lang="en">the number of expected resource partitions (i.e pages).</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#expectedTriples -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#expectedTriples">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">expected triples</rdfs:label>
        <rdfs:comment xml:lang="en">the expected maximum number of triple to be indexed by an activity.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#forceAccept -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#forceAccept">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">force accept</rdfs:label>
        <rdfs:comment xml:lang="en">Define specified http accept header in requesting source. The sintax must be complain to RFC 2616 for HTTP 1.1. accept header. If not specified &apos;application/n-triples,application/ld+json,application/rdf+xml,text/turtle,*/*;q=0.4&apos; it is used.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#fromPage -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#fromPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">from page</rdfs:label>
        <rdfs:comment xml:lang="en">The first page returned by a SPARQL in a query iteration. Defaults to 1 if not present.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#generatesGraphType -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#generatesGraphType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">generates  graph type</rdfs:label>
        <rdfs:comment xml:lang="en">The  described accrual policy has the objective to generate/maintain a knowledge graph of the related type. If the property is not specified, nor explicitely nor implicitely (by axioms), g0v-budget:LinkedDataGraph is inferred.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#hasFrequencyPeriod -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#hasFrequencyPeriod">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has frequency period</rdfs:label>
        <rdfs:comment xml:lang="en">Frequency period in seconds.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/TR/swbp-xsch-datatypes/#section-duration"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#hasLimit -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#hasLimit">
        <rdfs:label xml:lang="en">has limit</rdfs:label>
        <rdfs:comment xml:lang="en">the max size in RDF triple of a paged resource</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#hasOffset -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#hasOffset">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has offset</rdfs:label>
        <rdfs:comment xml:lang="en">first record ofset of a page.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#hasResilience -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#hasResilience">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has resilience</rdfs:label>
        <rdfs:comment xml:lang="en">The number of times  ABOX graph is retained in storage after a failure in fetchin it. The default is 0 that means that when a graph fails to load it is removed from the knowledge base. If the value of this attribute is, for instance 3, this means that the Graph will be keept in the knowledge base untill its loading fails for three consecutive  times (the fourth error drives a graph drop). A succesfull loading reset the fail counter.N.B. a &quot;not modified&quot; message does not reset the fail counter.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#havingTargetGraphName -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#havingTargetGraphName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">having target graph name</rdfs:label>
        <rdfs:comment xml:lang="en">States that the described ingestion plan is about the creation/maintenance of a graph named as the referenced object.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#includes -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#includes">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">include</rdfs:label>
        <rdfs:comment xml:lang="en">The URL of a web resource exposing a g0v-budget configuration data.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://data.budget.g0v.it/g0v-budget/v1#url"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#isLastPage -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#isLastPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">is the last page</rdfs:label>
        <rdfs:comment xml:lang="en">states that the referenced page is the last one. If not present defautlt is false.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#lodLaundromatUrl -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#lodLaundromatUrl">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">lod laundromat url</rdfs:label>
        <rdfs:comment xml:lang="en">An url of a resource stored in the  lod laundromat wardrobe. similar to llo:url.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://data.budget.g0v.it/g0v-budget/v1#url"/>
        <rdfs:seeAlso rdf:resource="http://lodlaundromat.org/"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#pageNum -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#pageNum">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">page number</rdfs:label>
        <rdfs:comment xml:lang="en">A page sequence number .</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#pageSize -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#pageSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">page size</rdfs:label>
        <rdfs:comment xml:lang="en">The size for a SPARQL query page. Equivalent  with LIMIT clause. Defaults to 1000 if not defined</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#password -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#password">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">password</rdfs:label>
        <rdfs:comment xml:lang="en">Password in basic autentication method</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#policyName -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#policyName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">policy name</rdfs:label>
        <rdfs:comment xml:lang="en">a conventional name for a Policy. Should be univoque inside the knowledge base.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#queryMethod -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#queryMethod">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">query method</rdfs:label>
        <rdfs:comment xml:lang="en">The http request method used in a SPARQL query. Accepted metods: &quot;POST&quot;  and &quot;GET&quot;. Defaults to &quot;POST&quot; if not specified.</rdfs:comment>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>GET</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>POST</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#queryName -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#queryName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">query name</rdfs:label>
        <rdfs:comment xml:lang="en">The name of the graph that will contain a sparql query result. If not specfied hinerit the sparql service endpoint url.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://data.budget.g0v.it/g0v-budget/v1#createsGraphName"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#queryText -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#queryText">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">query text</rdfs:label>
        <rdfs:comment xml:lang="en">the text of a sparql query. In the query text you can inject variables with the form %varname=default value% where varname is a word a default value a string.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#ruleName -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#ruleName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">rule  name</rdfs:label>
        <rdfs:comment xml:lang="en">a conventional name for a Reasoning Policy. Should be univoque inside the knowledge base.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://data.budget.g0v.it/g0v-budget/v1#policyName"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#ruleText -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#ruleText">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The rule text body in the form:
prefixes declaration
IF { sparql where clause }
THEN { sparql insert(delete clause }.

Example:
PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;
PREFIX gr: &lt;http://purl.org/goodrelations/v1#&gt;
PREFIX :&lt;urn:test:&gt;
IF {
           ?offering gr:hasPriceSpecification ?ps .
           ?ps gr:hasCurrencyValue ?price .
           FILTER (?price &gt;= 200.00).
}
THEN {
           ?offering a :ExpensiveProduct .
}

</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#source -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#source">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">source</rdfs:label>
        <rdfs:comment xml:lang="en">The URL of a web resource exposing RDF data facts.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://data.budget.g0v.it/g0v-budget/v1#url"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#sparqlEndpoint -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#sparqlEndpoint">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">sparql endpoint</rdfs:label>
        <rdfs:comment xml:lang="en">An url of a sparql service endpoint. similar to sd:endpoint</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://data.budget.g0v.it/g0v-budget/v1#url"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#supportsHttpCaching -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#supportsHttpCaching">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">supports http caching</rdfs:label>
        <rdfs:comment xml:lang="en">Support http caching specification (RFC 2616 ) in sending HTTP requests. If not present, true is inferred.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://data.budget.g0v.it/g0v-budget/v1#supportsLDPPP -->

    <owl:DatatypeProperty rdf:about="http://data.budget.g0v.it/g0v-budget/v1#supportsLDPPP">
