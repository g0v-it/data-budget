PREFIX : <urn:local:g0v:api:v1:>

SELECT ?top_partition ?amount
WHERE {
  ?accountUri a :AccountTopPartition;
                :amount ?amount;
                :label ?top_partition;
}ORDER BY DESC(?amount)


SELECT ?second_partition ?amount
WHERE {
  ?accountUri a :AccountSecondPartition;
                :amount ?amount;
                :label ?second_partition;
}ORDER BY DESC(?amount)