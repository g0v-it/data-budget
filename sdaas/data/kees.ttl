		resource:legge_bilancio_2018,
		resource:legge_bilancio_2018_schema,
		resource:rendiconto_2017,
		resource:rendiconto_2017_schema;
	void:uriSpace "http://data.budget.g0v.it/resource/";
	dct:subject <https://it.wikipedia.org/wiki/Bilancio_dello_Stato>	
.

